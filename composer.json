{
    "name": "phpnomad/framework",
    "description": "",
    "version": "1.0.0",
    "type": "library",
    "homepage": "https://github.com/phpnomad/core",
    "readme": "README.md",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix"
    },
    "autoload": {
        "psr-4": {
            "PHPNomad\\Framework\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPNomad\\Framework\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Standiford",
            "email": "alex@standiford.us"
        }
    ],
    "require": {
        "phpnomad/db": "^2.0",
        "phpnomad/di": "^1.0",
        "phpnomad/event": "^1.0",
        "phpnomad/rest": "^1.0",
        "phpnomad/logger": "^1.0"
    },
    "require-dev": {
        "phpnomad/tests": "^0.1.0 || ^0.3.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
